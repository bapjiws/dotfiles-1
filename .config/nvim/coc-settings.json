  "diagnostic.format": "%message [%source]",
  "diagnostic.enableSign": true,
  "diagnostic.errorSign": " ",
  "diagnostic.warningSign": " ",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": " ",
