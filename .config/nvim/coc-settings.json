{
  "explorer.width": 30,
  "explorer.icon.expanded": "▾",
  "explorer.icon.collapsed": "▸",
  "explorer.file.column.git.icon.modified": "•",
  "explorer.file.column.git.icon.deleted": "✖",
  "explorer.file.column.git.icon.untracked": "ᵁ",
  "explorer.file.column.git.icon.added": "✚",
  "explorer.file.column.git.icon.renamed": "≫",
  "explorer.file.column.git.icon.unmerged": "≠",
  "explorer.icon.enableNerdfont": true,
  //"explorer.icon.enableVimDevicons": true,
  "explorer.previewAction.onHover": false,
  "explorer.quitOnOpen": true,
  "explorer.openAction.for.directory": "expandOrCollapse",
  "explorer.keyMappings": {
    "<cr>": ["expandable?", "expand", "open"],
    "v": "open:vsplit"
  },
  //CocList
  "list.source.files.defaultOptions": ["--auto-preview"],
  "list.source.outline.defaultOptions": ["--auto-preview"],
  "list.indicator": "❯",
  "list.selectedSignText": " ",
  // siggestions
  "suggest.triggerAfterInsertEnter": false,
  "suggest.timeout": 500,
  "suggest.noselect": false,
  // "suggest.snippetIndicator": "👾",
  "suggest.triggerCompletionWait": 100,
  "suggest.autoTrigger": "always",
  "suggest.maxCompleteItemCount": 20,
  "suggest.removeDuplicateItems": true,
  "suggest.completionItemKindLabels": {
    "keyword": "",
    "variable": "識",
    "value": "\uf89f",
    "operator": "\u03a8",
    "function": "",
    "reference": "\ufa46",
    "constant": "識",
    "method": "",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "",
    "field": "\uf9be",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf723",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },
  // eslint
  //  "eslint.options": {
  //   "configFile": "/Users/void/.eslintrc.yml"
  //},
  "eslint.filetypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact",
    "typescript.tsx"
  ],
  //"eslint.autoFix": true,
  //"eslint.autoFixOnSave": true,
  //diagnostic
  "diagnostic.signOffset": 9999999,
  "diagnostic.highlightOffset": 9999999,
  "diagnostic.messageDelay": 500,
  //"diagnostic.errorSign": " ",
  "diagnostic.errorSign": " ",
  "diagnostic.warningSign": " ",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": " ",
  "diagnostic.checkCurrentLine": true,
  "diagnostic.refreshOnInsertMode": true,
  // preferences
  "coc.preferences.snippetStatusText": "Ⓢ ",
  "coc.preferences.formatOnType": true,
  "coc.preferences.extensionUpdateCheck": "daily",
  "coc.preferences.hoverTarget": "float",
  "coc.preferences.formatOnSaveFiletypes": [
    "markdown",
    "javascript",
    "javascriptreact",
    "typescript",
    "json",
    "html",
    "css",
    "scss",
    "yaml"
  ],
  //git
  //  "git.addGBlameToBufferVar": true,
  //  "git.addGBlameToVirtualText": true,
  //  "git.virtualTextPrefix": " ❯❯❯ ",
  "git.addedSign.hlGroup": "GitGutterAdd",
  "git.changedSign.hlGroup": "GitGutterChange",
  "git.removedSign.hlGroup": "GitGutterDelete",
  "git.topRemovedSign.hlGroup": "GitGutterDelete",
  "git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
  "git.addedSign.text": "▋",
  "git.changedSign.text": "▋",
  "git.removedSign.text": "▋",
  "git.topRemovedSign.text": "▔",
  "git.changeRemovedSign.text": "▋",
  "javascript.suggest.autoImports": true,
  "tsserver.formatOnType": true,
  "typescript.showUnused": false,
  "javascript.showUnused": false,
  "scss.validate": true,
  // prettier
  "prettier.statusItemText": "ⓟ ",
  "prettier.singleQuote": true,
  "prettier.jsxSingleQuote": true,
  "prettier.disableSuccessMessage": true,
  "prettier.eslintIntegration": true,
  // coc-snippet
  "snippets.extends": { "javascriptreact": ["javascript"] },
  "snippets.snipmate.enable": false,
  "snippets.loadFromExtensions": false,
  "snippets.enableStatusItem": false,
  // coc-actions
  "coc-actions.hideCursor": false,
  "yank.highlight.duration": 800,
  "yank.enableCompletion": false,
  "git.enableGutters": false
}

