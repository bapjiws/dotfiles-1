{
  "explorer.width": 40,
  //"explorer.icon.enableNerdfont": true,
  "explorer.icon.enableVimDevicons": true,
  "explorer.previewAction.onHover": false,
  "explorer.quitOnOpen": true,
  // "explorer.file.column.git.icon.added": "✚",
  //  "explorer.file.column.git.icon.deleted": "✖",
  //  "explorer.file.column.git.icon.renamed": "➜",
  //  "explorer.file.column.git.icon.unmerged": "═",
  //  "explorer.file.column.git.icon.untracked": "?",
  //  "explorer.file.column.git.icon.modified": "✹",
  //  "explorer.file.column.git.icon.ignored": "☒",
  "explorer.openAction.for.directory": "expandOrCollapse",
  "explorer.keyMappings": {
    "<cr>": ["expandable?", "expand", "open"],
    "v": "open:vsplit"
  },
  "list.indicator": ">",
  "list.selectedSignText": " ",
  "suggest.autoTrigger": "always",
  "suggest.maxCompleteItemCount": 20,
  "suggest.removeDuplicateItems": true,
  "suggest.completionItemKindLabels": {
    "keyword": "\uf1de",
    "variable": "\ue79b",
    "value": "\uf89f",
    "operator": "\u03a8",
    "function": "\u0192",
    "reference": "\ufa46",
    "constant": "\uf8fe",
    "method": "\uf09a",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "\ue624",
    "field": "\uf9be",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf723",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },
  "eslint.filetypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact",
    "typescript.tsx"
  ],
  "diagnostic.messageDelay": 500,
  //"diagnostic.errorSign": " ",
  "diagnostic.errorSign": " ",
  "diagnostic.warningSign": " ",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": " ",
  "diagnostic.checkCurrentLine": true,
  "coc.preferences.formatOnType": true,
  "coc.preferences.extensionUpdateCheck": "daily",
  "coc.preferences.hoverTarget": "float",
  "coc.preferences.formatOnSaveFiletypes": [
    "markdown",
    "javascript",
    "javascriptreact",
    "typescript",
    "json",
    "html",
    "css",
    "scss",
    "yaml"
  ],

  "javascript.suggest.autoImports": true,
  "tsserver.formatOnType": true,
  "typescript.showUnused": false,
  "javascript.showUnused": false,
  "scss.validate": true,
  "yank.highlight.duration": 800,
  "yank.enableCompletion": false,
  //  "git.addedSign.text": "┃",
  //  "git.removedSign.text": "┃",
  //  "git.changedSign.text": "┃",
  //  "git.changeRemovedSign.text": "┃",
  //  "git.topRemovedSign.text": "┃",
  //  "git.changedSign.hlGroup": "CocGitChange",
  //  "git.removedSign.hlGroup": "CocGitDelete",
  //  "git.addedSign.hlGroup": "CocGitAdd",
  // "git.changeRemovedSign.hlGroup": "CocGitChange",
  //  "git.topRemovedSign.hlGroup": "CocGitDelete",
  //  "eslint.options": {
  //   "configFile": "/Users/void/.eslintrc.yml"
  //},
  "prettier.singleQuote": true,
  "prettier.jsxSingleQuote": true,
  "prettier.statusItemText": "",
  "prettier.disableSuccessMessage": true,
  "prettier.eslintIntegration": true,
  "snippets.extends": { "javascriptreact": ["javascript"] },
  "snippets.snipmate.enable": false,
  "snippets.loadFromExtensions": false,
  "snippets.enableStatusItem": false
  //  "git.enableGutters": false
}
