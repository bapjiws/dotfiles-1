{
  "coc.preferences.colorSupport": true,
  "coc.preferences.hoverTarget": "echo",

  "coc-actions.hideCursor": false,

  "suggest.completionItemKindLabels": {
    "keyword": "",
    "variable": "識",
    "value": "\uf89f",
